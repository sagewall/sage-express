doctype html
html
  head
    title= title
    meta(name='viewport', content='initial-scale=1,maximum-scale=1,user-scalable=no')
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    block stylesheets
  body
    nav.navbar.navbar-inverse
      .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='#navbar')
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') SageWall.io
        #navbar.collapse.navbar-collapse
          ul.nav.navbar-nav
            li
              a(href='/resume') Resume
            li
              a(href='/projects') Projects
            li
              a(href='/weather') Weather
            li(class='dropdown')
              a(href='#', class='dropdown-toggle', data-toggle='dropdown' role='button' aria-haspopup='true' aria-expanded='false') ESRI Maps
                span(class='caret')
              ul(class='dropdown-menu')
                li
                  a(href='/darkskies') Dark Skies Map
                li
                  a(href='/snowfall') Snowfall Map
                li
                  a(href='/hazards') Hazards Map
            li(class='dropdown')
              a(href='#', class='dropdown-toggle', data-toggle='dropdown' role='button' aria-haspopup='true' aria-expanded='false') Leaflet Maps
                span(class='caret')
              ul(class='dropdown-menu')
                li
                  a(href='/historic') Historic Places
            li(class='dropdown')
              a(href='#', class='dropdown-toggle', data-toggle='dropdown' role='button' aria-haspopup='true' aria-expanded='false') Mapbox Maps
                span(class='caret')
              ul(class='dropdown-menu')
                li
                  a(href='/parcels') Parcels

          ul.nav.navbar-nav.navbar-right
            li
              a(href='/mongo')
                span.glyphicon.glyphicon-user
                span Login

    block content
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.12.2/jquery.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js')
    script(src='/javascripts/analytics.js')
    block javascripts
